<%= render "layouts/sidebar" %>
    <main class="py-10 lg:pl-72">
      <%= form_for ([:admin, @course]) do |f| %>
        <div class="field">
          <%= f.label :name %><br />
          <%= f.text_field :name, autofocus: true %>
        </div>
        <div class="field">
          <%= f.label :address %><br />
          <%= f.text_field :address %>
        </div>
        <div class="field">
          <%= f.label :city %><br />
          <%= f.text_field :city %>
        </div>
        <div class="field">
          <%= f.label :latitude %><br />
          <%= f.text_field :latitude %>
        </div>
        <div class="field">
          <%= f.label :longitude %><br />
          <%= f.text_field :longitude %>
        </div>
        <div class="field">
          <%= f.label :country %><br />
          <%= f.text_field :country %>
        </div>
        <div class="field">
          <%= f.label :fairway_grass %><br />
          <%= f.text_field :fairway_grass %>
        </div>
        <div class="field">
          <%= f.label :green_grass %><br />
          <%= f.text_field :green_grass %>
        </div>
        <div class="field">
          <%= f.label :number_of_holes %><br />
          <%= f.text_field :number_of_holes %>
        </div>
        <div class="field">
          <%= f.label :length_format %><br />
          <%= f.text_field :length_format %>
        </div>
        <div class="field">
          <%= f.label :phone %><br />
          <%= f.text_field :phone %>
        </div>
        <div class="field">
          <%= f.label :state %><br />
          <%= f.text_field :state %>
        </div>
        <div class="field">
          <%= f.label :website %><br />
          <%= f.text_field :website %>
        </div>
        <div class="field">
          <%= f.label :zip %><br />
          <%= f.text_field :zip %>
        </div>

        <%= f.submit 'Create course' %>
      <% end %>
    </main>